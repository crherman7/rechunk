{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project."
    },
    "readKey": {
      "type": "string",
      "description": "The read key for accessing the project."
    },
    "writeKey": {
      "type": "string",
      "description": "The write key for accessing the project."
    },
    "publicKey": {
      "type": "string",
      "description": "The public key associated with the project."
    },
    "privateKey": {
      "type": "string",
      "description": "The private key associated with the project."
    },
    "entry": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      },
      "description": "Entries associated with the project."
    },
    "external": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "External resources related to the project."
    },
    "host": {
      "type": "string",
      "description": "The host for the project."
    }
  },
  "required": [
    "project",
    "readKey",
    "writeKey",
    "publicKey",
    "privateKey",
    "entry",
    "external",
    "host"
  ]
}
